    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "reactify": "^1.1.1",
    "request": "^2.83.0",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "rootDir": ".",
    "roots": [
      "<rootDir>/src/"
    ],
    "cacheDirectory": "<rootDir>/.jest/",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\](?!vendavo-*).+\\.(js|jsx)$"
    ],
    "timers": "fake",
    "testURL": "http://localhost/#",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  }
}
