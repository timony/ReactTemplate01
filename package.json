    "babel-register": "^6.26.0",
    "babelify": "^8.0.0",
    "browserify": "^15.2.0",
    "eslint": "^4.15.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-plugin-react": "^7.5.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.2.0",
    "gulp-eslint": "^4.0.1",
    "gulp-if-else": "^1.0.3",
    "gulp-inject": "^4.3.0",
    "gulp-notify": "^3.2.0",
